{
    "compilerOptions": {
        "moduleDetection": "force",
        "module": "Preserve",
        "resolveJsonModule": true,
        "allowJs": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "outDir": "./dist"
    },
    "exclude": [
        "./dist/**/*",
        "./types/**/*"
    ]
}